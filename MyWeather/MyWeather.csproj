<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>weatherICO.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="weatherICO.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="8.0.12" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.Sqlite" Version="8.0.12" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
    <PackageReference Include="Syncfusion.Licensing" Version="28.1.41" />
    <PackageReference Include="syncfusion.ui.wpf.net" Version="28.1.41" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="WeatherHist.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <Page Update="WeatherHist.xaml">
      <SubType>Designer</SubType>
    </Page>
  </ItemGroup>

</Project>
